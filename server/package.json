{
	"name": "server",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"start": "npm run prod",
		"server": "babel-node ./src/bin/www",
		"server:dev": "npm build:grammar  && babel-node ./src/bin/dev",
		"server:prod": "node ./dist/bin/www",
		"dev": "SET NODE_ENV=development& npm-run-all server:dev",
		"clean": "rimraf dist",
		"build": "npm build:grammar  && babel ./src --out-dir dist",
		"build:grammar": "nearleyc ./src/routes/bundler/scripts/object.ne -o nearleyc ./src/routes/bundler/scripts/object.js",
		"prod": "SET NODE_ENV=production& npm-run-all clean build server:prod",
		"watch": "nodemon"
	},
	"dependencies": {
		"@babel/plugin-proposal-class-properties": "^7.5.5",
		"cookie-parser": "~1.4.4",
		"debug": "~2.6.9",
		"express": "~4.16.1",
		"js-levenshtein": "^1.1.6",
		"morgan": "~1.9.1",
		"mysql": "^2.17.1",
		"nearley": "^2.19.1",
		"node-fetch": "^2.6.0",
		"npm-run-all": "^4.1.5",
		"rimraf": "^3.0.0",
		"sse-broadcast": "^0.5.2"
	},
	"devDependencies": {
		"@babel/cli": "^7.6.4",
		"@babel/core": "^7.6.4",
		"@babel/node": "^7.6.3",
		"@babel/preset-env": "^7.6.3",
		"nodemon": "^1.19.3",
		"prettier": "^1.19.1"
	}
}
