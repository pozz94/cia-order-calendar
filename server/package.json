{
	"name": "server",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"start": "npm run prod",
		"server": "babel-node ./src/bin/www",
		"server:dev": "babel-node ./src/bin/dev",
		"server:prod": "node ./dist/bin/www",
		"predev": "npm run build:grammar && npm run build:router",
		"dev": "SET NODE_ENV=development& npm run server:dev",
		"clean": "rimraf dist",
		"prebuild": "npm run build:grammar && npm run build:router",
		"build": "webpack --config webpack.config.js",
		"postbuild": "copy dist\\*.* ..\\build && xcopy /S /Q /I src\\serviceScripts ..\\build\\serviceScripts",
		"build:grammar": "nearleyc ./src/routes/bundler/scripts/bundleGrammar.ne -o ./src/routes/bundler/scripts/bundleGrammar.js",
		"build:router": "babel-node ./src/bin/prebuild",
		"prod": "SET NODE_ENV=production& npm-run-all clean build server:prod",
		"watch": "nodemon"
	},
	"dependencies": {
		"cookie-parser": "~1.4.4",
		"debug": "~2.6.9",
		"express": "~4.16.1",
		"js-levenshtein": "^1.1.6",
		"lodash": "^4.17.15",
		"moo": "^0.5.1",
		"morgan": "~1.9.1",
		"mysql": "^2.17.1",
		"nearley": "^2.19.1",
		"node-cron": "^2.0.3",
		"node-fetch": "^2.6.0",
		"npm-run-all": "^4.1.5",
		"rimraf": "^3.0.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.6.4",
		"@babel/core": "^7.6.4",
		"@babel/node": "^7.6.3",
		"@babel/plugin-proposal-class-properties": "^7.5.5",
		"@babel/preset-env": "^7.6.3",
		"babel-loader": "^8.0.6",
		"generate-package-json-webpack-plugin": "^1.0.1",
		"nodemon": "^1.19.3",
		"prettier": "^1.19.1",
		"webpack": "^4.41.6",
		"webpack-cli": "^3.3.12",
		"webpack-node-externals": "^1.7.2"
	}
}
